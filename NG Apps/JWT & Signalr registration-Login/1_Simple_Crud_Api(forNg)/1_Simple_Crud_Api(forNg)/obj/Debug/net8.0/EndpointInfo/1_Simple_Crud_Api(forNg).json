  "components": {
    "schemas": {
      "Student": {
        "type": "object",
        "properties": {
          "grNumber": {
            "type": "integer",
            "format": "int32"
          },
          "sname": {
            "type": "string",
            "nullable": true
          },
          "sgender": {
            "type": "string",
            "nullable": true
          },
          "sage": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "sstd": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "sphoto": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Teacher": {
        "required": [
          "password",
          "userName"
        ],
        "type": "object",
        "properties": {
          "teacherId": {
            "type": "integer",
            "format": "int32"
          },
          "userName": {
            "minLength": 1,
            "type": "string"
          },
          "password": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "TeacherDto": {
        "required": [
          "password",
          "userName"
        ],
        "type": "object",
        "properties": {
          "userName": {
            "minLength": 1,
            "type": "string"
          },
          "password": {
            "minLength": 6,
            "type": "string"
          }
        },
        "additionalProperties": false
      }
    },
    "securitySchemes": {
      "Bearer": {
        "type": "http",
        "description": "Please enter a valid token. Example: 'Bearer <JWT_token>'",
        "scheme": "Bearer",
        "bearerFormat": "JWT"
      }
    }
  },
  "security": [
    {
      "Bearer": [ ]
    }
  ]
}